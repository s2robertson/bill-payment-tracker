<div class="col">
    <div class="col-md-9 col-lg-8 col-xl-7 col-xxl-6">
        <form>
            <h2>Sign up</h2>
            <div class="mb-3">
                <label for="signupUsername" class="form-label">Log-in name:</label>
                <input id="signupUsername" type="text" class="form-control">
            </div>
            <div class="mb-3">
                <label for="signupEmail" class="form-label">Email:</label>
                <input id="signupEmail" type="email" class="form-control">
            </div>
            <div class="mb-3">
                <label for="signupPassword" class="form-label">Password:</label>
                <input id="signupPassword" type="password" class="form-control">
            </div>
            <div class="mb-3">
                <button id="signupSubmit" type="submit" class="btn">Sign Up</button>
            </div>
            <p id="signupFormFeedback" class="form-text"></p>
        </form>
    </div>
</div>
<script src="/js/registerScript.js"></script>